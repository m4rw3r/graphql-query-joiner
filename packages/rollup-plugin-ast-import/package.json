{
  "name": "@awardit/rollup-plugin-graphql-ast-import",
  "version": "1.0.0",
  "description": "Rollup plugin enabling imports of operations as AST from .graphql files.",
  "keywords": [
    "rollup-plugin",
    "import",
    "graphql",
    "ast"
  ],
  "authors": [
    "Martin Wernst√•l <m4rw3r@gmail.com>"
  ],
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "pnpm run build:d.ts && pnpm run build:js",
    "build:d.ts": "rollup -c",
    "build:js": "tsc --project tsconfig.build.json",
    "check": "tsc --noEmit",
    "clean": "rm -rf coverage dist",
    "prepack": "pnpm run clean && pnpm run check && pnpm run test && pnpm run build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.3.0",
    "graphql": "^16.12.0"
  }
}
